{
  "name": "frontend-new",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:backend": "cd backend && npm run dev",
    "dev:full": "concurrently \"pnpm run dev\" \"npm run dev:backend\"",
    "dev:db": "docker-compose -f docker-compose.dev.yml up -d postgres",
    "dev:db:down": "docker-compose -f docker-compose.dev.yml down",
    "dev:db:local": "echo 'PostgreSQL должен быть запущен локально на порту 5432'",
    "dev:full:with-db": "npm run dev:db && sleep 5 && concurrently \"npm run dev\" \"npm run dev:backend\"",
    "dev:full:local": "concurrently \"npm run dev\" \"npm run dev:backend\"",
    "db:setup": "npm run dev:db && sleep 5 && cd backend && npm run db:generate && npm run db:push",
    "db:setup:local": "cd backend && npm run db:generate && npm run db:push",
    "build": "tsc -b && vite build",
    "build:prod": "tsc -b && vite build --config vite.config.prod.ts",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@reduxjs/toolkit": "^2.8.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "countries-list": "^3.1.1",
    "lucide-react": "^0.535.0",
    "moment": "^2.30.1",
    "react": "^19.1.1",
    "react-big-calendar": "^1.19.4",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.61.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.7.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.9",
    "@types/react-big-calendar": "^1.16.2",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "sass": "^1.89.2",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^7.0.6"
  }
}
